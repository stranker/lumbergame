script/source = "extends Node

var wood_count : int = 0
var gold_count : int = 0
var trees_chopped : int = 0

export (PackedScene) var tree_scene
export (PackedScene) var wooden_log_scene
export (PackedScene) var particles_scene

var current_tree

signal chop_tree(wood_count)
signal tree_down(trees_chopped)
signal new_tree(tree)
signal on_gold_changed(gold_count)
signal on_wood_changed(wood_count)

func _ready():
	spawn_new_tree()
	pass

func spawn_new_tree():
	var new_tree = tree_scene.instance()
	$World.call_deferred('add_child',new_tree)
	new_tree.translation = $World/TreePos.translation
	current_tree = new_tree
	new_tree.connect('tree_down', self, 'on_tree_down')
	new_tree.connect('on_chopped', self, 'on_tree_chopped')
	emit_signal('new_tree',new_tree)
	pass

func _on_ClickArea_gui_input(event):
	if event is InputEventScreenTouch and !event.is_echo() and event.pressed:
		if current_tree and current_tree.is_alive():
			wood_count += 1
			emit_signal('chop_tree', wood_count)
			current_tree.take_chop(1)
			spawn_wooden_log()
			spawn_particles()
	pass # Replace with function body.

func spawn_particles():
	var part = particles_scene.instance()
	part.translation = $World/TreePos/ChopParticles.translation
	$World/TreePos/ChopParticles.call_deferred('add_child',part)
	pass

func spawn_wooden_log():
	var new_wlog = wooden_log_scene.instance()
	call_deferred('add_child',new_wlog)
	new_wlog.translation = $World/WLPos.translation
	pass

func on_tree_chopped(health):
	$GUI.on_tree_chopped(health)
	pass

func on_tree_down():
	trees_chopped += 1
	emit_signal('tree_down', trees_chopped)
	spawn_new_tree()
	pass # Replace with function body.


func _on_ExitButton_button_down():
	get_tree().quit()
	pass # Replace with function body.


func _on_Shop_sell_wooden():
	gold_count += wood_count
	wood_count = 0
	emit_signal('on_gold_changed', gold_count)
	emit_signal('on_wood_changed', wood_count)
	pass # Replace with function body.
"

