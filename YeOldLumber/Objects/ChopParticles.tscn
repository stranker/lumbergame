[sub_resource type="GDScript" id=1]
script/source = "extends Spatial

func _ready():
	transparency()
	pass

func transparency():
	var new_mat = $Particles.mesh.surface_get_material(0).duplicate()
	new_mat.flags_transparent = true
	$Particles.material_override = new_mat
	var init_color = new_mat.albedo_color
	var final_color = init_color
	final_color.a = 0
	$Tween.interpolate_property(new_mat,'albedo_color', init_color,
	final_color, 0.6, Tween.TRANS_LINEAR, Tween.EASE_OUT, 0.4)
	$Tween.start()
	pass

func _on_Timer_timeout():
	call_deferred('queue_free')
	pass # Replace with function body.
"

